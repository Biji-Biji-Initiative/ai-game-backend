{
  "name": "responses-api-fight-club",
  "version": "1.0.0",
  "description": "A stateful web app that challenges users with dynamic, AI-generated cognitive challenges to highlight their unique Human Edge",
  "main": "index.js",
  "bin": {
    "responses-cli": "./cli.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "cli": "node cli.js start",
    "test": "node scripts/run-tests.js",
    "test:watch": "mocha 'src/test/**/*.js' --recursive --watch",
    "test:user": "mocha 'src/test/user/**/*.js'",
    "test:personality": "mocha 'src/test/personality/**/*.js'",
    "test:unit": "mocha \"tests/unit/**/*.test.js\" --recursive",
    "test:domains": "mocha --require ./tests/helpers/globalSetup.js tests/domains/**/*.test.js",
    "test:domain:challenge": "mocha \"tests/domain/challenge/**/*.test.js\" --recursive",
    "test:domain:focusArea": "mocha \"tests/domain/focusArea/**/*.test.js\" --recursive",
    "test:domain:evaluation": "mocha \"tests/domain/evaluation/**/*.test.js\" --recursive",
    "test:domain:prompt": "mocha \"tests/domain/prompt/**/*.test.js\" --recursive",
    "test:domain:user": "mocha \"tests/domain/user/**/*.test.js\" --recursive",
    "test:domain:personality": "mocha \"tests/domain/personality/**/*.test.js\" --recursive",
    "test:infrastructure": "mocha --require ./tests/helpers/globalSetup.js tests/infrastructure/**/*.test.js",
    "test:infrastructure:services": "mocha --require ./tests/helpers/globalSetup.js tests/infrastructure/services/**/*.test.js",
    "test:integration": "mocha \"tests/integration/**/*.test.js\" --recursive",
    "test:integration:flow": "mocha --require ./tests/helpers/globalSetup.js tests/integration/openai-supabase-flow.test.js",
    "test:integration:openai-supabase": "mocha --require ./tests/helpers/globalSetup.js tests/integration/openai-supabase-flow.test.js",
    "test:integration:focus-area": "mocha --require ./tests/helpers/globalSetup.js tests/integration/focus-area-flow.test.js",
    "test:integration:evaluation": "mocha --require ./tests/helpers/globalSetup.js tests/integration/evaluation-flow.test.js",
    "test:integration:prompt": "mocha --require ./tests/helpers/globalSetup.js tests/integration/prompt-flow.test.js",
    "test:integration:user": "mocha --require ./tests/helpers/globalSetup.js tests/integration/user-flow.test.js",
    "test:integration:personality-user": "mocha --require ./tests/helpers/globalSetup.js tests/integration/personalityUserIntegration.test.js",
    "test:integration:challenge-evaluation": "mocha --require ./tests/helpers/globalSetup.js tests/integration/challenge-evaluation-flow.test.js",
    "test:integration:run-all": "node scripts/run-all-integration-tests.js",
    "test:integration:with-checks": "./scripts/run-tests-with-db-check.sh",
    "test:unit:user": "mocha --require ./tests/helpers/globalSetup.js tests/unit/user/**/*.test.js",
    "test:unit:personality": "mocha --require ./tests/helpers/globalSetup.js tests/unit/personality/**/*.test.js",
    "test:api": "mocha \"tests/api/**/*.test.js\" --recursive",
    "test:api:user": "mocha --require ./tests/helpers/globalSetup.js tests/api/user/**/*.test.js",
    "test:api:personality": "mocha --require ./tests/helpers/globalSetup.js tests/api/personality/**/*.test.js",
    "test:api:cross-domain": "mocha --require ./tests/helpers/globalSetup.js tests/api/cross-domain/**/*.test.js",
    "test:external": "mocha \"tests/external/**/*.test.js\" --recursive",
    "test:external:openai": "mocha --require ./tests/helpers/globalSetup.js tests/external/openai/**/*.test.js",
    "test:external:supabase": "mocha --require ./tests/helpers/globalSetup.js tests/external/supabase/**/*.test.js",
    "test:external:openai:logs": "node tests/external/openai/test-direct-api-logs.js",
    "test:external:supabase:logs": "node tests/external/supabase/test-direct-db-logs.js",
    "test:e2e": "mocha \"tests/e2e/**/*.test.js\" --recursive",
    "test:shared": "mocha --require ./tests/helpers/globalSetup.js tests/shared/**/*.test.js",
    "test:bdd": "cucumber-js",
    "test:bdd:smoke": "cucumber-js -p smoke",
    "test:bdd:verbose": "cucumber-js -p verbose",
    "test:bdd:responses": "cucumber-js -p responses",
    "test:run": "node scripts/run-tests.js",
    "dev:challenge": "node dev-scripts/test-challenge-generation.js",
    "dev:evaluation": "node dev-scripts/test-responses-api.js",
    "dev:prompt": "node dev-scripts/test-prompt-builder.js",
    "dev:auth": "node dev-scripts/test-auth.js",
    "dev:openai": "node dev-scripts/test-openai-eval.js",
    "dev:flow": "node dev-scripts/test-evaluation-flow.js",
    "dev:view-evals": "node dev-scripts/view-evaluations.js",
    "migrate": "npx supabase migration new",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:migrate": "node scripts/run-migrations.js",
    "db:init-challenge-config": "node scripts/init-challenge-config.js",
    "setup:supabase": "node scripts/create-supabase-tables.js",
    "setup:env": "node scripts/setup-test-env.js",
    "setup:test-user": "node scripts/create-test-user.js",
    "get:token": "node scripts/get-token.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "lint:imports": "eslint src/ --rule 'no-restricted-imports: error' --rule 'no-restricted-modules: error'",
    "lint:jsdoc": "eslint src/ --rule 'jsdoc/require-jsdoc: error'",
    "test:api:focus-area": "mocha --require ./tests/helpers/globalSetup.js tests/api/focusArea/**/*.test.js",
    "test:api:challenge": "mocha --require ./tests/helpers/globalSetup.js tests/api/challenge/**/*.test.js",
    "setup:test": "node scripts/setup-test-env.js",
    "test:domain": "mocha \"tests/domain/**/*.test.js\" --recursive",
    "test:application": "mocha \"tests/application/**/*.test.js\" --recursive",
    "test:with-env": "mocha --require ./tests/loadEnv.js",
    "test:domain:with-env": "mocha --require ./tests/loadEnv.js \"tests/domain/**/*.test.js\" --recursive",
    "test:integration:with-env": "mocha --require ./tests/loadEnv.js \"tests/integration/**/*.test.js\" --recursive",
    "test:external:with-env": "mocha --require ./tests/loadEnv.js \"tests/external/**/*.test.js\" --recursive",
    "test:external:openai:with-env": "mocha --require ./tests/loadEnv.js \"tests/external/openai/**/*.test.js\" --recursive",
    "test:external:openai:direct": "mocha --require ./tests/loadEnv.js \"tests/external/openai/direct/**/*.test.js\" --recursive",
    "test:external:openai:responses": "mocha --require ./tests/loadEnv.js \"tests/external/openai/responses-api/**/*.test.js\" --recursive",
    "test:e2e:with-env": "mocha --require ./tests/loadEnv.js \"tests/e2e/**/*.test.js\" --recursive",
    "test:challenge": "mocha --require ./tests/loadEnv.js \"tests/integration/challenge/**/*.test.js\" \"tests/external/openai/direct/challengeGeneration.direct.test.js\" --recursive",
    "test:focusArea": "mocha --require ./tests/loadEnv.js \"tests/integration/focusArea/**/*.test.js\" --recursive",
    "test:runner": "node scripts/run-tests.js",
    "test:runner:domain": "node scripts/run-tests.js --domain",
    "test:runner:integration": "node scripts/run-tests.js --integration",
    "test:runner:external": "node scripts/run-tests.js --external",
    "test:runner:e2e": "node scripts/run-tests.js --e2e",
    "test:runner:challenge": "node scripts/run-tests.js --focus=challenge",
    "test:runner:focusArea": "node scripts/run-tests.js --focus=focusArea",
    "test:runner:evaluation": "node scripts/run-tests.js --focus=evaluation",
    "test:runner:skip-openai": "node scripts/run-tests.js --skip-openai",
    "test:migrate": "node scripts/migrate-tests.js",
    "test:migrate:fix": "node scripts/migrate-tests.js --fix",
    "test:reorganize": "node scripts/reorganize-tests.js",
    "test:reorganize:move": "node scripts/reorganize-tests.js --move",
    "test:check-legacy": "node scripts/reorganize-tests.js tests/legacy/**/*.js",
    "test:cleanup": "node scripts/clean-up-old-tests.js",
    "prepare:test:env": "cp .env.test.example .env.test && echo 'Created .env.test from example. Update with your credentials.'",
    "test:runner:unit": "node scripts/run-tests.js --unit",
    "test:working-examples": "mocha --require ./tests/loadEnv.js tests/unit/environment-test.test.js tests/external/openai/responses-api/sample-responses-api.test.js",
    "quality": "node scripts/enforce-code-quality.js",
    "quality:fix": "node scripts/enforce-code-quality.js --fix",
    "quality:jsdoc": "node scripts/enforce-code-quality.js --jsdoc-only"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testEnvironment": "node",
    "testTimeout": 15000,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/archive/"
    ]
  },
  "keywords": [
    "ai",
    "openai",
    "responses-api",
    "cognitive-challenges",
    "human-edge"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "openai": "^3.3.0",
    "readline": "^1.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "chai": "^4.5.0",
    "chai-as-promised": "^8.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "mkdirp": "^3.0.1",
    "mocha": "^10.8.2",
    "mock-fs": "^5.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "supabase": "^2.20.5",
    "supertest": "^6.3.3"
  }
}
