{
  "name": "responses-api-fight-club",
  "version": "1.0.0",
  "description": "A stateful web app that challenges users with dynamic, AI-generated cognitive challenges to highlight their unique Human Edge",
  "main": "index.js",
  "bin": {
    "responses-cli": "./cli.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "cli": "node cli.js start",
    "test": "jest",
    "test:unit": "jest --testPathPattern=src",
    "test:domains": "mocha --require ./tests/helpers/globalSetup.js tests/domains/**/*.test.js",
    "test:domain:challenge": "mocha --require ./tests/helpers/globalSetup.js tests/domains/challenge/**/*.test.js",
    "test:domain:focusArea": "mocha --require ./tests/helpers/globalSetup.js tests/domains/focusArea/**/*.test.js",
    "test:domain:evaluation": "mocha --require ./tests/helpers/globalSetup.js tests/domains/evaluation/**/*.test.js",
    "test:domain:prompt": "mocha --require ./tests/helpers/globalSetup.js tests/domains/prompt/**/*.test.js",
    "test:domain:user": "mocha --require ./tests/helpers/globalSetup.js tests/domains/user/**/*.test.js",
    "test:integration": "mocha --require ./tests/helpers/globalSetup.js tests/integration/**/*.test.js",
    "test:integration:flow": "mocha --require ./tests/helpers/globalSetup.js tests/integration/openai-supabase-flow.test.js",
    "test:integration:openai-supabase": "mocha --require ./tests/helpers/globalSetup.js tests/integration/openai-supabase-flow.test.js",
    "test:integration:focus-area": "mocha --require ./tests/helpers/globalSetup.js tests/integration/focus-area-flow.test.js",
    "test:integration:evaluation": "mocha --require ./tests/helpers/globalSetup.js tests/integration/evaluation-flow.test.js",
    "test:integration:prompt": "mocha --require ./tests/helpers/globalSetup.js tests/integration/prompt-flow.test.js",
    "test:integration:user": "mocha --require ./tests/helpers/globalSetup.js tests/integration/user-flow.test.js",
    "test:integration:challenge-evaluation": "mocha --require ./tests/helpers/globalSetup.js tests/integration/challenge-evaluation-flow.test.js",
    "test:integration:run-all": "node scripts/run-all-integration-tests.js",
    "test:integration:with-checks": "./scripts/run-tests-with-db-check.sh",
    "test:external": "mocha --require ./tests/helpers/globalSetup.js tests/external/**/*.test.js",
    "test:external:openai": "mocha --require ./tests/helpers/globalSetup.js tests/external/openai/**/*.test.js",
    "test:external:supabase": "mocha --require ./tests/helpers/globalSetup.js tests/external/supabase/**/*.test.js",
    "test:e2e": "cucumber-js || mocha --require ./tests/helpers/globalSetup.js tests/e2e/**/*.test.js",
    "test:shared": "mocha --require ./tests/helpers/globalSetup.js tests/shared/**/*.test.js",
    "test:real-api": "mocha --require ./tests/helpers/globalSetup.js tests/real-api/**/*.test.js",
    "test:real-api:focus-area": "mocha --require ./tests/helpers/globalSetup.js tests/real-api/focusArea/**/*.test.js",
    "test:real-api:challenge": "mocha --require ./tests/helpers/globalSetup.js tests/real-api/challenge/**/*.test.js",
    "test:openai:logs": "node tests/real-api/openai/test-direct-api-logs.js",
    "test:supabase:logs": "node tests/real-api/supabase/test-direct-db-logs.js",
    "test:bdd": "cucumber-js",
    "test:bdd:smoke": "cucumber-js -p smoke",
    "test:bdd:verbose": "cucumber-js -p verbose",
    "test:bdd:responses": "cucumber-js -p responses",
    "test:setup": "node scripts/create-test-dirs.js",
    "test:cleanup": "node scripts/cleanup-test-files.js",
    "test:remove-old": "node scripts/remove-old-tests.js",
    "migrate": "node scripts/run-migrations.js",
    "migrate:eval-categories": "node scripts/run-migrations.js evaluation_category_mappings.sql",
    "setup:supabase": "node scripts/create-supabase-tables.js",
    "view:evaluations": "node scripts/view-evaluations.js",
    "test:move-legacy": "node scripts/move-legacy-tests.js",
    "test:cleanup-legacy": "node scripts/cleanup-legacy-dirs.js",
    "test:check-structure": "node scripts/test-structure-check.js",
    "setup:env": "node scripts/update-env.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testEnvironment": "node",
    "testTimeout": 15000,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/archive/"
    ]
  },
  "keywords": [
    "ai",
    "openai",
    "responses-api",
    "cognitive-challenges",
    "human-edge"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "openai": "^4.89.0",
    "readline": "^1.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "chai": "^4.5.0",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "mock-fs": "^5.5.0",
    "nodemon": "^3.0.1",
    "sinon": "^17.0.1"
  }
}